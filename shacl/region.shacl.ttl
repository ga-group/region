<> sh:entailment <http://www.w3.org/ns/entailment/RDFS> .

rgn:region.shape a sh:NodeShape ;
	sh:targetClass lcc-cr:GeographicRegion ;
	## tests
	sh:property rgn:comp.label ;
	sh:property rgn:comp.classified ;
	.

rgn:comp.label a sh:PropertyShape ;
	sh:path rdfs:label ;
	sh:minCount 1 ;
	sh:type xsd:string ;
	sh:uniqueLang true ;
	.

rgn:comp.classified a sh:PropertyShape ;
	sh:path lcc-cr:isClassifiedBy ;
	sh:minCount 1 ;
	sh:class lcc-cr:GeographicRegionKind ;
	.
